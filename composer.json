{
    "name": "remediation/maxit",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Remediation\\Maxit\\": "src/",
            "App\\Entity\\": "src/entity/",
            "App\\Service\\": "src/service/",
            "App\\Repository\\": "src/repository/",
            "App\\Controller\\": "src/controller/",
            "App\\Core\\": "app/core/",
            "App\\Core\\Abstract\\": "app/core/abstract/",
            "App\\Core\\Middlewares\\": "app/core/middlewares/",
            "App\\Migration\\": "migrations/",
            "App\\Seeders\\": "seeders/"
        }
    },
        "scripts":{
        "database:migrate": "@php bin/migrate",
        "database:seed": "@php bin/seed"

    },

    "bin":["bin/migrate"],
    
    "authors": [
        {
            "name": "dieng"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "symfony/yaml": "^7.3"
        }
    }
    
